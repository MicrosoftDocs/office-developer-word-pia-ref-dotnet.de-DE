    <Member MemberName="RectangleType">
      <MemberSignature Language="C#" Value="public Microsoft.Office.Interop.Word.WdRectangleType RectangleType { get; }" />
      <MemberSignature Language="ILAsm" Value=".property instance valuetype Microsoft.Office.Interop.Word.WdRectangleType RectangleType" />
